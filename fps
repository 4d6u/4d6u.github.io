return (function()local a=workspace:FindFirstChildOfClass("Terrain")local b=game:GetService("Lighting")local c=game:GetService("RunService")a.WaterWaveSize=0;a.WaterWaveSpeed=0;a.WaterReflectance=0;a.WaterTransparency=0;b.GlobalShadows=false;b.FogEnd=9e9;b.Brightness=0;settings().Rendering.QualityLevel=1;for d,e in pairs(game:GetDescendants())do if e:IsA("Part")or e:IsA("UnionOperation")or e:IsA("MeshPart")or e:IsA("CornerWedgePart")or e:IsA("TrussPart")then e.Material="Plastic"e.Reflectance=0 elseif e:IsA("Decal")then e.Transparency=1 elseif e:IsA("ParticleEmitter")or e:IsA("Trail")then e.Lifetime=NumberRange.new(0)elseif e:IsA("Explosion")then e.BlastPressure=1;e.BlastRadius=1 elseif e:IsA("Beam")then e.Enabled=false end end;for d,e in pairs(b:GetDescendants())do if e:IsA("BlurEffect")or e:IsA("SunRaysEffect")or e:IsA("ColorCorrectionEffect")or e:IsA("BloomEffect")or e:IsA("DepthOfFieldEffect")then e.Enabled=false end end;workspace.DescendantAdded:Connect(function(f)task.spawn(function()if f:IsA("ForceField")or f:IsA("Sparkles")or f:IsA("Smoke")or f:IsA("Fire")then c.Heartbeat:Wait()f:Destroy()end end)end)if setfflag then setfflag("MovePrerender2",false)setfflag("DebugTextureManagerSkipMips",-1)setfflag("RenderCSGTriangles",false)setfflag("DisableCSM",true)setfflag("ParticlePhysics",false)setfflag("LightingTechnology","Voxel")end;workspace.StreamingEnabled=true;local sethiddenproperty=sethiddenproperty or set_hidden_property;if sethiddenproperty then sethiddenproperty(b,"Technology","Voxel")sethiddenproperty(game:GetService("Workspace"),"MeshPartHeadsAndAccessories",1)sethiddenproperty(game:GetService("Workspace"),"PhysicsSteppingMethod","Fixed")end;if setfpscap then setfpscap(9e9)end end)()
