--!optimize 2
--!nonstrict
return(function() local a=workspace:FindFirstChildOfClass('Terrain')local b=game:GetService('Lighting')local c=game:GetService('RunService')a.WaterWaveSize=0;a.WaterWaveSpeed=0;a.WaterReflectance=0;a.WaterTransparency=0;b.GlobalShadows=false;b.FogEnd=9e9;settings().Rendering.QualityLevel=1;for d,e in pairs(game:GetDescendants())do if e:IsA("Part")or e:IsA("UnionOperation")or e:IsA("MeshPart")or e:IsA("CornerWedgePart")or e:IsA("TrussPart")then e.Material="Plastic"e.Reflectance=0 elseif e:IsA("Decal")then e.Transparency=1 elseif e:IsA("ParticleEmitter")or e:IsA("Trail")then e.Lifetime=NumberRange.new(0)elseif e:IsA("Explosion")then e.BlastPressure=1;e.BlastRadius=1 end end;for d,e in pairs(b:GetDescendants())do if e:IsA("BlurEffect")or e:IsA("SunRaysEffect")or e:IsA("ColorCorrectionEffect")or e:IsA("BloomEffect")or e:IsA("DepthOfFieldEffect")then e.Enabled=false end end;workspace.DescendantAdded:Connect(function(f)task.spawn(function()if f:IsA('ForceField')then c.Heartbeat:Wait()f:Destroy()elseif f:IsA('Sparkles')then c.Heartbeat:Wait()f:Destroy()elseif f:IsA('Smoke')or f:IsA('Fire')then c.Heartbeat:Wait()f:Destroy()end end)end)if setfflag then setfflag("MovePrerender2",false)setfflag("DebugTextureManagerSkipMips",-1)end;workspace.StreamingEnabled=true;workspace.StreamingMinRadius=32;workspace.StreamingTargetRadius=64;local sethiddenproperty=sethiddenproperty or set_hidden_property;if sethiddenproperty then sethiddenproperty(workspace.Terrain,"Decoration",false)sethiddenproperty(b,"Technology","Voxel")end;if setfpscap then setfpscap(9e9)end end)()
